.skills-row {
  font-weight: bold;
  display: flex;
  flex-direction: column;

  &__title {
    font-size: 24px;
    color: #100022;
    margin-bottom: 5%;
  }

  &__list {
    display: inline-flex;
    justify-content: space-between;
    font-size: 18px;
    color: rgba(16, 0, 34, 0.5);
    margin-bottom: 5%;
  }
  
  &__item {
    margin-right: 50px;
    
    &:last-child {
      margin-right: 0;
    }
  }

}